{
    "name": "asdoria/sylius-product-document-bundle",
    "type": "symfony-bundle",
    "description": "Asdoria Product Document",
    "license": "MIT",
    "keywords": [
        "product",
        "asdoria",
        "sylius",
        "sylius-plugin",
        "document"],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        "asdoria/media-bundle": "^0.5",
        "sylius/product-bundle": "^1.8",
        "sylius/sylius": "^1.9",
        "symfony/dotenv": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "sort-packages": true,
        "gitlab-domains": [
            "gitlab.asdoria.org"
        ]
    },
    "repositories": [
        {
            "type": "gitlab",
            "url": "https://gitlab.asdoria.org/asdoria/asdoriabundles/AsdoriaMediaBundle"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.10-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Asdoria\\SyliusProductDocumentPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Asdoria\\SyliusProductDocumentPlugin\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php bin/create_node_symlink.php"
        ],
        "post-update-cmd": [
            "php bin/create_node_symlink.php"
        ],
        "post-create-project-cmd": [
            "php bin/create_node_symlink.php"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
